        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.16.0",
        "babel-preset-stage-1": "^6.24.1",
        "babel-register": "^6.26.0",
        "babel-runtime": "^6.26.0",
        "body-parser": "^1.18.1",
        "bourbon": "~5.0.1",
