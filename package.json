{
    "name": "veams-react-movie-list",
    "description": "Custom project description",
    "version": "0.0.1",
    "engines": {
        "node": ">=6.0.0"
    },
    "scripts": {
        "clear:app": "rimraf app/**",
        "local:server": "cross-env BABEL_ENV=server nodemon src/server/index.js --config ./configs/tasks/nodemon.config.json",
        "pre-start": "clear:app",
        "start": "cross-env NODE_ENV=local npm-run-all --parallel local:*",
        "dev:build": "cross-env NODE_ENV=dev npm run build",
        "qa:build": "cross-env NODE_ENV=qa npm run build",
        "prod:build": "cross-env NODE_ENV=production npm run build",
        "local:start": "webpack --mode=development --watch --hide-modules",
        "build": "npm run clear:app && cross-env BABEL_ENV=client webpack --mode=production",
        "sprite:generate": "node configs/tasks/icons/sprite.js",
        "grunticon:generate": "node configs/tasks/icons/grunticon.js",
        "images:generate": "node configs/tasks/images/images-resizer.js",
        "faker": "node configs/tasks/faker/faker.js",
        "lint:styles": "stylelint src/app/**/*.scss --config configs/tasks/linting/stylelint.config.js",
        "lint:scripts": "eslint \"src/**/*.js\" --config configs/tasks/linting/eslint.config.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint:styles -- --fix && npm run lint:styles && npm run lint:scripts -- --fix && npm run lint:scripts"
        }
    },
    "dependencies": {
        "@veams/base": "^1.0.3",
        "@veams/core": "^1.0.0",
        "@veams/helpers": "^1.2.8",
        "@veams/http-service": "^1.0.2",
        "@veams/plugin-logger": "^1.0.0",
        "@veams/plugin-media-query-handler": "^1.0.0",
        "@veams/plugin-vent": "^1.0.0",
        "axios": "^0.18.0",
        "history": "^4.7.2",
        "lazysizes": "^4.0.4",
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "react-redux": "^5.0.7",
        "react-router-config": "^1.0.0-beta.4",
        "react-router-dom": "^4.3.1",
        "react-router-redux": "^5.0.0-alpha.9",
        "redux": "^4.0.0",
        "redux-devtools-extension": "^2.13.5",
        "redux-immutable-state-invariant": "^2.1.0",
        "redux-observable": "^0.19.0",
        "redux-promise": "^0.6.0",
        "reselect": "^3.0.1",
        "rxjs": "^5.5.2"
    },
